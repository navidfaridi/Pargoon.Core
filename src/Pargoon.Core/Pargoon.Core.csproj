<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<TargetFramework>netstandard2.1</TargetFramework>
	<Nullable>enable</Nullable>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<PackageProjectUrl>https://github.com/navidfaridi/Pargoon.Core/</PackageProjectUrl>
	<FileVersion>1.1.9</FileVersion>
	<Version>1.1.9</Version>
	<Copyright>Navid Faridi</Copyright>
	<Description>Pargoon.Core is a core library that provides various helpful class for web Api and more.</Description>
	<AssemblyVersion>1.1.9</AssemblyVersion>
	<PackageReleaseNotes>Add JsonConvert on top of system.text.json with default serilizing and deserializing camelCase,CaseInsensitive and UnsafeRelaxedJsonEscaping options</PackageReleaseNotes>
	<Authors>Navid Faridi</Authors>
	<SignAssembly>False</SignAssembly>
	<RepositoryUrl>https://github.com/navidfaridi/Pargoon.Core/tree/main/src/Pargoon.Core</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
	<LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="8.0.4" />
  </ItemGroup>
  
</Project>
